<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".gameresults">

    <TextView
        android:id="@+id/textViewspeler1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Speler1"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@+id/btn_Results"
        tools:layout_editor_absoluteX="26dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textViewspeler2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:text="Speler2"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewspeler1"
        app:layout_constraintStart_toEndOf="@+id/textViewspeler1"
        app:layout_constraintTop_toBottomOf="@+id/btn_Results"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tVAntwoordR1S1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.519"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tVAntwoordR1S2"
        android:layout_width="7dp"
        android:layout_height="15dp"
        android:text=""

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.736"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.518"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tVAntwoordR2S1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="148dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tVAntwoordR1S1"
        app:layout_constraintVertical_bias="0.525"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tVAntwoordR2S2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""

        app:layout_constraintBottom_toTopOf="@+id/tVAntwoordR3S2"
        app:layout_constraintTop_toBottomOf="@+id/tVAntwoordR1S2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="305dp" />

    <TextView
        android:id="@+id/tVAntwoordR3S1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="148dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tVAntwoordR2S1"
        app:layout_constraintVertical_bias="0.917"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tVAntwoordR3S2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="558dp" />

    <Button
        android:id="@+id/btn_Results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#D00101"
        android:text="Result"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />
</androidx.constraintlayout.widget.ConstraintLayout>